{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "importHelpers": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2015",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "json-schema-form": [
        "dist/json-schema-form/json-schema-form",
        "dist/json-schema-form"
      ],
      "json-schema-form2": [
        "dist/json-schema-form2/json-schema-form2",
        "dist/json-schema-form2"
      ],
      "json-edit": [
        "dist/json-edit/json-edit",
        "dist/json-edit"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}